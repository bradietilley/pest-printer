{
    "name": "bradietilley/pest-printer",
    "description": "A PestPHP printer",
    "type": "library",
    "license": "MIT",
    "require": {
        "pestphp/pest": "^1.22",
        "nunomaduro/termwind": "^1.13",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.22",
        "orchestra/testbench": "^7.19",
        "symfony/var-dumper": "^6.1.5",
        "laravel/pint": "^1.4",
        "phpstan/phpstan": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "BradieTilley\\PestPrinter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Bradie Tilley",
            "homepage": "https://github.com/bradietilley"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BradieTilley\\PestPrinter\\PestPrinterServiceProvider"
            ]
        }
    }
}
